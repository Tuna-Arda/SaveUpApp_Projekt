<?xml version="1.0" encoding="utf-8" ?>
<Shell
    x:Class="SaveUpApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:SaveUpApp.Views">

    <!-- ShellContent (optional) -->
    <ShellContent
        Route="HomePage"
        ContentTemplate="{DataTemplate views:HomePage}" />
    <ShellContent
        Route="AddProductPage"
        ContentTemplate="{DataTemplate views:AddProductPage}" />
    <ShellContent
        Route="ListPage"
        ContentTemplate="{DataTemplate views:ListPage}" />
    
</Shell>
